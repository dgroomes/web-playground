{
  "name": "Message Passing",
  "manifest_version": 2,
  "description": "This is a simple extension from the 'web-playground/browser-extension' subproject",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://localhost/*"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ]
}
